
head
    script(src="https://code.jquery.com/jquery-2.2.4.js", integrity="sha256-iT6Q9iMJYuQiMWNd9lDyBUStIq/8PuOW33aOqmvFpqI=", crossorigin="anonymous")
body   
    ul
        each item in a
            - var roomno=item.roomno
            - var details=item.detail
            - var imageurl=item.imgurl
            - var issues=item.issues
            - var complaint_number=item.complaint_number 
            - var complaint_status=item.issue_status 
            - var lol2 = item.imgurl
            - var lol3 = "uploads/"+item.imgurl
            - var lol = {"src" : lol3}            
                li
                form(action='', method='POST')
                    label Room Number
                    p(type='text')=roomno
                    label Details
                    p(type='text')=details
                    label Isuues
                    p(type='text')=issues
                    label ImageUrl
                    p(type='text')=complaint_number
                    label Complaint Status
                    p(type='text')=complaint_status
                    button(type='submit', value="complaint Solved", id=complaint_number,onclick="",name="state")  complaint_solved                  
                li
                    img&attributes(lol)

        script
            include  cp3.js        
